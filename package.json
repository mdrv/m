{
	"name": "@mdrv/m",
	"version": "257.3.1",
	"description": "ðŸª´ Take your JS/TS existing code to the minimalist approach.",
	"keywords": [
		"helpers",
		"generics",
		"stdlib",
		"toolkit",
		"types",
		"utilities"
	],
	"homepage": "https://github.com/mdrv/m",
	"bugs": {
		"url": "https://github.com/mdrv/m/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mdrv/m.git"
	},
	"license": "GPL-3.0",
	"author": "Umar Alfarouk <medrivia@gmail.com> (https://alfarouk.id)",
	"type": "module",
	"exports": {
		".": "./src/v257/index.ts",
		"./dom": "./src/v257/dom.ts",
		"./node": "./src/v257/node.ts",
		"./*/dom": "./src/*/dom.ts",
		"./*/node": "./src/*/node.ts",
		"./*": "./src/*/index.ts"
	},
	"files": ["dist"],
	"scripts": {
		"b": "tsdown",
		"f": "dprint fmt",
		"fc": "dprint check"
	},
	"publishConfig": {
		"main": "./dist/v257/index.js",
		"types": "./dist/v257/index.d.ts",
		"exports": {
			".": {
				"default": "./dist/v257/index.js",
				"types": "./dist/v257/index.d.ts"
			},
			"./dom": {
				"default": "./dist/v257/dom.js",
				"types": "./dist/v257/dom.d.ts"
			},
			"./node": {
				"default": "./dist/v257/node.js",
				"types": "./dist/v257/node.d.ts"
			},
			"./*/dom": {
				"default": "./dist/*/dom.js",
				"types": "./dist/*/dom.d.ts"
			},
			"./*/node": {
				"default": "./dist/*/node.js",
				"types": "./dist/*/node.d.ts"
			},
			"./*": {
				"default": "./dist/*/index.js",
				"types": "./dist/*/index.d.ts"
			}
		}
	},
	"devDependencies": {
		"@types/bun": "^1.2.18",
		"@types/node": "^24.0.12",
		"tsdown": "^0.12.9"
	},
	"dependencies": {
		"loglevel": "^1.9.2"
	}
}
